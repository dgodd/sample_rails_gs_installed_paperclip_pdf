<p id="notice"><%= notice %></p>

<h1>Listing Files Apps</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @files_apps.each do |files_app| %>
      <tr>
        <td><%= link_to 'Show', files_app %></td>
        <td><%= link_to 'Edit', edit_files_app_path(files_app) %></td>
        <td><%= link_to 'Destroy', files_app, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Files app', new_files_app_path %>
